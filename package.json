{
  "name": "eligius-examples",
  "version": "1.0.0",
  "description": "Example presentations of the Eligius engine",
  "main": "docs/index.index.html",
  "author": "Roland Zwaga <rbzwaga@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "eligius": "link:../eligius/dist",
    "jquery": "3.6.1",
    "lottie-web": "5.9.6",
    "monaco-editor": "0.34.1",
    "video.js": "8.0.4"
  },
  "scripts": {
    "config-example-1": "ts-node ./examples/requestanimationframe/config/generate-configuration",
    "bundle-example-1": "ts-node ./src/engine-pack-builder ./examples/requestanimationframe/src/config-data.json && node esbuild-1.js",
    "build-example-1": "yarn config-example-1 && yarn bundle-example-1",
    "config-example-2": "ts-node ./examples/videojs/config/generate-configuration",
    "bundle-example-2": "ts-node ./src/engine-pack-builder ./examples/videojs/src/config-data.json && node esbuild-2.js",
    "build-example-2": "yarn config-example-2 && yarn bundle-example-2",
    "build": "yarn build-example-1 && yarn build-example-2",
    "clean": "npx rimraf docs/requestanimationframe & npx rimraf docs/videojs"
  },
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "0.5.0",
    "@types/fs-extra": "11.0.1",
    "@types/node": "18.13.0",
    "@types/uuid": "9.0.0",
    "esbuild": "0.17.8",
    "esbuild-plugin-ignore": "1.1.1",
    "fs-extra": "11.1.0",
    "ts-morph": "17.0.1",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "uuid": "9.0.0"
  }
}
